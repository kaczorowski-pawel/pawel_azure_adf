{
	"name": "02_stores_from_csv_to_sql_pool",
	"properties": {
		"activities": [
			{
				"name": "StoresDataLake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowCopyCommand": true
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "StoreKey",
									"type": "String"
								},
								"sink": {
									"name": "StoreKey",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "GeographyKey",
									"type": "String"
								},
								"sink": {
									"name": "GeographyKey",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "StoreName",
									"type": "String"
								},
								"sink": {
									"name": "StoreName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Status",
									"type": "String"
								},
								"sink": {
									"name": "Status_name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "CloseReason",
									"type": "String"
								},
								"sink": {
									"name": "CloseReason",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "EmployeeCount",
									"type": "String"
								},
								"sink": {
									"name": "EmployeeCount",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "SellingAreaSize",
									"type": "String"
								},
								"sink": {
									"name": "SellingArea",
									"type": "Int32"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "CSVDataToCopy",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "asa_sql_pool",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}